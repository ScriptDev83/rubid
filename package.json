{
  "name": "rubidium",
  "version": "0.0.0",
  "description": "A small unique job scheduler.",
  "main": "index.js",
  "scripts": {
    "test": "jshint *.js && tape test"
  },
  "devDependencies": {
    "tape": "^2.10.2"
  },
  "keywords": [
    "cron",
    "schedule",
    "tasks"
  ],
  "author": "Mark Stanley Everitt",
  "license": "MIT",
  "jshintConfig": {
    "bitwise": true,
    "camelcase": true,
    "curly": true,
    "eqeqeq": true,
    "forin": false,
    "immed": true,
    "indent": 4,
    "latedef": true,
    "newcap": true,
    "noarg": true,
    "noempty": true,
    "nonew": true,
    "plusplus": false,
    "quotmark": "single",
    "undef": true,
    "unused": true,
    "strict": true,
    "trailing": true,
    "maxparams": 4,
    "maxdepth": 3,
    "maxstatements": 15,
    "maxcomplexity": 11,
    "maxlen": 100,
    "smarttabs": true,
    "node": true,
    "browser": true,
    "esnext": true
  }
}
