{
  "name": "rubidium",
  "version": "0.1.1",
  "description": "A small unique job scheduler.",
  "main": "index.js",
  "scripts": {
    "test": "jshint *.js && nodeunit test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/qubyte/rubidium"
  },
  "dependencies": {
    "json-stable-stringify": "0.1.1"
  },
  "devDependencies": {
    "nodeunit": "0.8.6",
    "jshint": "2.4.4"
  },
  "keywords": [
    "cron",
    "schedule",
    "tasks",
    "atd"
  ],
  "author": "Mark Stanley Everitt",
  "license": "MIT",
  "jshintConfig": {
    "bitwise": true,
    "camelcase": true,
    "curly": true,
    "eqeqeq": true,
    "forin": false,
    "immed": true,
    "indent": 4,
    "latedef": true,
    "newcap": true,
    "noarg": true,
    "noempty": true,
    "nonew": true,
    "plusplus": false,
    "quotmark": "single",
    "undef": true,
    "unused": true,
    "strict": true,
    "trailing": true,
    "maxparams": 4,
    "maxdepth": 3,
    "maxstatements": 15,
    "maxcomplexity": 11,
    "maxlen": 100,
    "smarttabs": true,
    "node": true,
    "browser": true,
    "esnext": true
  }
}
